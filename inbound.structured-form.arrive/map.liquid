{
    "dtos": [
        {
            "type": "{{input.MessageProperties.EventType}}",
            "data": {
                "MessageGuid": "{{input.MessageProperties.MessageId}}",
                "ParentMessageGuid": "{{input.MessageProperties.AuditId}}",
                "MessageContentType": "{{input.MessageProperties.EventType}}",
                "MessageContent": {
                    "FormId": "{{ input.FormId }}",
                    "Content": [],
                    "Coordinates": {
                        "Latitude": {
                            { input.Content.Data.taskData.startedLocation.lat
                            }
                        },
                        "Longitude": {
                            { input.Content.Data.taskData.startedLocation.lon
                            }
                        }
                    },
                    "Assets": {
                        { assets
                        }
                    },
                    "CreateDate": "{{input.CreateDate | date: "%Y-%m-%dT%H:%M:%S+00: 00" }}"
                }
            }
        },
        {
            "type": "{{input.MessageProperties.EventType}}",
            "data": {
                "MessageGuid": "{{ reverseMsgId }}",
                "ParentMessageGuid": "{{input.MessageProperties.AuditId}}",
                "MessageContentType": "{{input.MessageProperties.EventType}}",
                "MessageContent": {
                    "TripStatusType": 3,
                    "TripStatusReason": 0,
                    "Location": {
                        "Latitude": {
                            { lat
                            }
                        },
                        "Longitude": {
                            { lon
                            }
                        }
                    },
                    "Odometer": 0.0,
                    "TripInformation": {
                        "TripName": null,
                        "Move": null,
                        "Leg": 8440
                        },
                        "Stop": 1
                        },
                        "OrderHeader": 0.0,
                        "Assets": [],
                        "AdditionalDataElements": []
                    },
                    "HeaderInformation": {
                        "CreateDateUtc": "{{input.CreateDate | date: "%Y-%m-%dT%H:%M:%S+00: 00" }}"
                    }
                }
            }
        }
    ]
}